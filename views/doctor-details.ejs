						<div class="tg-doctor-detail tg-doctor-detail2 tg-haslayout">
							<article class="tg-doctor-profile">
									<div class="tg-box">										
											<figure class="tg-docprofile-img">
												<span class="tg-featuredicon"><em class="fa fa-bolt"></em></span>
												<img src="<%= userInfo.profile_pic_url %>" class="pp" alt="image description">						
												<!--<img src="/assets/images/icons/offline-red-icon.png" class="presence"><span class="on"></span>-->
											</figure>																						
											<div class="tg-docprofile-content">
												<div class="tg-heading-border tg-small">
													<h3>Dr. <%= userInfo.firstname %> <%= userInfo.lastname %> <%= userInfo.title %></h3>
													<h4><%= userInfo.specialty %> Specialist</h4>
												</div>												
												<ul class="tg-doccontactinfo">
													<li>
														<i class="fa fa-map-marker"></i>
														<span style="font-weight: bold"><%= userInfo.work_place %>|</span>
														<address><%= userInfo.address %></address>
													</li>
													<li>
														<i class="fa fa-phone"></i>
														<span>+234 <%= userInfo.phone %></span>
													</li>
													<li>
														<i class="fa fa-envelope-o"></i>
														<a href="obinna@fareland.com">jhon@company.com</a>
													</li>
													<li>														
														<span style="font-weight: bold">Residence: </span>
														<span><%= userInfo.city %>, <%= userInfo.country || "N/A" %></span>
													</li>
													<li>
														<span style="font-weight: bold">Experience: </span>
														<span><%= userInfo.experience || "N/A" %> years</span>
													</li>
													<li>
														<span style="font-weight: bold">Degree(s): </span>
														<% for(var j = 0; j < userInfo.education.length; j++) { %>
															<span><%= userInfo.education[j].certificate %>,</span>
														<% } %>														
													</li>
                                                    
												</ul>
												<div class="tg-doc-feature">
												<div class="tg-heading-border tg-small">
													<h5>Sub Specialty</h5>
												</div>
												<div class="tg-description">
													<ul>
				                                    <% for(var i = 0; i < userInfo.sub_specialty.length; i++) { %>									  
													  	<li><%= userInfo.sub_specialty.sub_specialty || "not supplied" %></li>									  
				                                    <% } %>
													</ul>
												</div>
												<div class="tg-heading-border tg-small">
													<h5>Education(s)</h5>
												</div>
												<div class="tg-description">
													<ul>
				                                    <% for(var i = 0; i < userInfo.education.length; i++) { %>									  
													  	<li><%= userInfo.education[i].school %></li>
                                                        <span>Certification: <%= userInfo.education[i].certificate %></span>                                                     								  
				                                    <% } %>
													</ul>
												</div>										
												
												<div class="tg-heading-border tg-small">
													<h5>Office hours</h5>
												</div>
												<div class="tg-description">
													<ul>
				                                    <% for(var i = 0; i < userInfo.office_hour.length; i++) { %>									  
													  	<li><%= userInfo.office_hour[i].day %> <%= userInfo.office_hour[i].from %> to <%= userInfo.office_hour[i].to %></li>									  
				                                    <% } %>
													</ul>
												</div>
												<div class="tg-heading-border tg-small">
													<h5>Honors and Awards</h5>
												</div>
												<div class="tg-description">
													<ul>
				                                    <% for(var i = 0; i < userInfo.sub_specialty.length; i++) { %>									  
													  	<li><%= userInfo.sub_specialty.sub_specialty || "not supplied" %></li>									  
				                                    <% } %>
													</ul>
												</div>								
												
												
											</div>							                                               
											</div>
										</div>
										<div class="communication-btn" ng-controller="bookController">                                                
										<button class="ng-show" ng-click="book(<%= userInfo.user_id %>)">BOOK NOW</button>
                                        <button class="ng-show" ng-click="ask(<%= userInfo.user_id %>)">ASK QUESTION</button>
                                        </div>	
									</article>							
							
                            					
