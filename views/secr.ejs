    <script type="text/ng-template" id='bank-info.html'>
         <div class="modal fade">
          <div class="modal-dialog">
            <div class="modal-content">
              <div class="modal-header" style="text-align:right">
                <i class="fa fa-times" style="font-size:18px;padding;20px" data-dismiss="modal"></i>
              </div>              
              <div class="modal-body"> 
                <h4 style="text-align:center">{{name}}</h4>
                  <input type="text" placeholder="bank name" ng-model="account.bank">
                  <input type="text" placeholder="acc no" ng-model="account.number">
                  <div style="text-align:center">
                    <button class="btn btn-primary" ng-click="cashout()">SEND</button>
                  </div>
              </div>
              <div class="modal-footer">
                
                        
              </div>
            </div>
          </div>
        </div>
     </script>

        
