        <main ng-controller="secrController">
            
            <section class="container">
                <header>
                    <h5>Service Notification</h5>
                </header>
                <div class='row'>
                    <div class="col-sm-12 col-md-5 col-lg-5 pull-right">
                        <div style="max-height: 500px;overflow-y: auto;">
                            <input type="text" placeholder="user id" ng-model="service.id" style="width: 60%;">
                            <button ng-click="viewService()" style="color:#fff" class="btn btn-primary"> User Id </button>
                        </div>                        
                    </div>
                    <div class="col-sm-12 col-md-7 col-lg-7 ">
                        <div style="max-height: 400px;overflow-y: auto;">
                            <div>
                                <article>
                                    <h5>{{service.center_name}}</h5>
                                    <address>
                                        <span>{{service.center_city}}</span>
                                        <span>{{service.center_country}}</span>
                                        <i class="fa fa-phone"> &nbsp; {{service.center_phone}}</i>
                                    </address>
                                    <ul>
                                        <li></li>
                                    </ul>
                                </article>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div style="border:bottom: 1px solid rgba(0,0,0,0.4);padding:20px">
                        <h6>{{m.name}}</h6>
                        <div>
                            <strong>{{m.amount}}</strong>
                        </div>
                        <div>
                            <span>{{m.firstname}} {{m.lastname}}</span>
                        </div>
                         <div>
                            <i class="fa fa-phone"> </i> <span> {{m.phone}}</span>
                        </div>
                    </div>
                </div>
            </section>
        </main>
        <div ng-controller="createRoomSecr"></div>
