        <main ng-controller="secrController">
            
            <section class="container">
                <header>
                    <h5>Service Notification</h5>
                </header>
                <div class='row'>
                    <div class="col-sm-12 col-md-5 col-lg-5 pull-right">
                        <div style="max-height: 500px;overflow-y: auto;">
                            <input type="text" placeholder=" enter user id" ng-model="service.id" style="width: 60%;">
                            <button ng-click="viewService()" style="color:#fff" class="btn btn-primary"> User Id </button>
                            <ul>
                                <li ng-repeat="j in servicelist" style="position:relative;border-bottom: 1px solid rgba(0,0,0,0.3)">{{j.message}}<span style="text-decoration:underline;color:red;font-size:18px;display:inline-block;position: absolute; right:5px" ng-click="viewService(j.centerId)">view</span></li>
                            </ul>
                        </div>                        
                    </div>
                    <div class="col-sm-12 col-md-7 col-lg-7">
                        <div style="max-height: 400px;overflow-y: auto;">
                            <div>
                                <article>
                                    <h5 style="color:red">{{sList.center_name}}</h5>
                                    <address>
                                        <span>{{sList.center_address}}</span>
                                        <span>{{sList.center_city}}</span>
                                        <span>{{sList.center_country}}</span>
                                        <div>
                                            <i class="fa fa-phone" ng-if="sList.center_name"> &nbsp; {{sList.center_phone}}</i>
                                        </div>
                                    <ul style="margin-top: 20px">
                                        <li style="color: green;text-decoration:underline" ng-if="sList.center_name">Unavailable services</li>
                                        <li ng-repeat="service in sList.unavailable_services"> @ {{service.name}}</li>
                                    </ul>
                                </article>
                            </div>
                        </div>
                    </div>
                </div>
                <div style="border-bottom: 1px solid #000;margin-top: 30px"></div>
                <div class="row">
                    <div style="border:bottom: 1px solid rgba(0,0,0,0.4);padding:20px;position:relative;border-bottom: 1px solid rgba(0,0,0,0.3);width:80%;margin:0 auto" ng-repeat = "m in mList">
                        <h6>{{m.name}}</h6>
                        <button class="btn btn-primary" ng-click="pay(m.user_id,m.name)" style="position:absolute;right:80px;bottom:20px">Pay</button>
                        <div>
                            <strong>{{m.ewallet.available_amount}}</strong>                            
                        </div>

                        <div>
                            <span>{{m.firstname}} {{m.lastname}}</span>
                        </div>
                         <div>
                            <i class="fa fa-phone"> </i> <span> {{m.phone}}</span>
                        </div>
                    </div>
                </div>
                
            </section>
        </main>
        <div ng-controller="createRoomSecr">
            <div>
                <button class="btn btn-primary" ng-click="check()">Check</button>
            </div>
        </div>
