        <main id="main" class="tg-main-section tg-haslayout" ng-controller="joinRoomController3">
            <div class="container-fluid">
                 <div class="request-popup" ng-class="{'pop-out':isReceivedRequest === true}">
                    <img ng-src="{{sender.profile_pic_url}}" style="width:32px; height:32px">
                    <div style="display:inline-block;padding:10px">
                        <strong style="width:80%;display:block;">{{sender.firstname}}!</strong>
                        <i class="fa fa-video-camera" style="color:#33430e;font-size: 18px;" ng-if="sender.type === 'Video Call'"> &nbsp;<span style="color#000">{{sender.type}} Request</span> </i>
                        <i class="fa fa-phone" style="color:#33430e;font-size: 18px;" ng-if="sender.type === 'Audio Call'"> &nbsp;<span style="color#000">{{sender.type}} Request</span> </i>
                        <i class="fa fa-calendar" aria-hidden="true" style="color:#33430e;font-size: 18px;" ng-if="sender.type === 'Meet In-Person'"> &nbsp;<span style="color#000">{{sender.type}} Request</span> </i>
                    </div>
                </div>
                <div class="row">
                    <div class="col-lg-9 col-md-9 col-sm-8 col-xs-12 pull-right">
                        <div class="row">
                            <div class="col-sm-12 col-md-9 col-lg-9">

                                <ul style='list-style:none'>
                                    <li ng-repeat="u in message1">
                                        <div ng-if="u.sent" ng-class="{'isYou' : u.sent}">
                                            <!--<p style="text-align:center"> &nbsp;{{u.time | date: "shortTime"}} &nbsp;{{u.time | date: "mediumDate"}}</p> -->  
                                            <article style="text-align:right;background-color:#d2ed88" class="chat">
                                                <span>{{u.sent}}</span>
                                                <small style="font-size:9px;font-style:italic"> &nbsp;{{u.time | date: "shortTime"}}</small>    
                                            </article>
                                        </div>
                                        <div ng-if="u.received" class="chatPallet">                                                         
                                            <article class="chat">
                                                <span>{{u.received}}</span>
                                                <small style="font-size:9px;font-style:italic"> &nbsp;{{u.time | date: "shortTime"}}</small>    
                                            </article>
                                        </div>
                                    </li>
                                   
                                </ul>                               
                                 
                            <div>
                                <article style="color:#000;margin-left: 18px;margin-top:10px;color:#eee;font-style:italic" ng-if="typing" class="chat">{{typing}}</article>                            
                                <input type="text" placeholder="Write..." ng-model="user.text3">
                                <button ng-click="sendChat3()">Send</button>
                            </div>
                            </div>
                            <button ng-click="online()">Online</button>
                            <button ng-click="offline()">Offline</button>
                            <button ng-click="doctorResponse()">Doc response</button>
                            <!--<my-iframe ng-src="{{link}}" height= "800" width="800">-->
                            
                           
                            <!--<div class="col-sm-3 col-md-3 col-lg-3">
                            </div>-->
                        </div>
                    </div>
                    
                   
                </div>
            </div><!-- main content -->
           
        </main>
