                <div class="tab-content tg-haslayout">
                    <div role="tabpanel" class="tab-pane tg-haslayout active" id="tg-signin-formarea" ng-controller="loginController">
                        <form class="tg-form-modal tg-form-signin">
                        <span style="color:red" ng-cloak>{{error}}</span>
                            <fieldset>
                                <div class="form-group">
                                    <input type="email" class="form-control" placeholder="Email Address" name="email" ng-model="login.email">
                                </div>
                                <div class="form-group">
                                    <input type="password" class="form-control" placeholder="Password" name="password" ng-model="login.password">
                                </div>                                
                                <div class="form-group tg-checkbox">
                                    <label>
                                        <input type="checkbox" class="form-control">
                                        Remember Me
                                    </label>
                                    <a class="tg-forgot-password" href="#">
                                        <i>Forgot Password</i>
                                        <i class="fa fa-question-circle"></i>
                                    </a>
                                </div>
                                <button class="tg-btn tg-btn-lg" ng-click="send()">LOGIN now</button>
                            </fieldset>
