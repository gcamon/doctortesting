            <!--************************************
                            Main Start
            *************************************-->
            <main id="main" class="tg-haslayout" ng-controller="authChangeController" style="max-height:800px;margin:100px 0" id="main">
                <section class="container">
                    <div class="row">
                        <div class="col-sm-12 col-md-12 col-lg-12">                           
                           <h2>Update Email Address</h2> 
                            <p>User verification pin has been sent to {{phone}} via SMS</p>
                            <form>
                                <div>
                                    <label>Enter verification pin (get from your phone) </label>
                                    <input type="text" ng-model="user.pin" placeholder="enter pin" class="form-control">
                                </div>
                                 <div>
                                    <label>New email address</label>
                                    <input type="text" ng-model="user.email" placeholder="enter new number" class="form-control">
                                </div>
                                 <div>
                                    <button class="btn btn-success pull-right" ng-click="update()">Submit</button>
                                </div>
                            </form>
                            <p>Having problem with the authentication process? <a href="/contact" style="text-decoration:none;color:green">Contact us for help</a></p>
                        </div>                                        
                    </div>
                </section>
            </main>
