    <script type="text/ng-template" id="granted-request.html">
         <div class="modal fade">
          <div class="modal-dialog">
            <div class="modal-content">
              <div class="modal-header">
                <h4 style="text-align: center; border-bottom: 1px solid rgba(0,0,0,0.3)">Acceptance from Dr. {{docName}}</h4>
              </div>
              <p style="color:red; text-align: center">{{message}}</p>
              <div class="modal-body">
                <h6 style="text-align:center">You wish to send acceptance notification</h6>
                <h5 style="text-align: center">To</h5>
                <h5 style="text-align: center">{{data.sender_firstname}} {{data.sender_lastname}} (patient)</h5>
                <p style="text-align: center">{{data.firstname}} {{data.lastname}}</p>
                <form>
                <span>Your consultation fee(amount) below</span><input type="number" placeholder="$ enter your consultation fee amount.*" ng-model="user.fee" required>
                <span>Total bill to patient including service charge:</a> {{total}}</span>
                <span style="font-size: 12px; color: rgba(0,0,0,0.7);display:block;padding:5px 0px"> Note: total bill is what the patient will see <a href="#" style="color: rgba(0,200,0,0.6)">learn more</a></span>
                <button class="tg-btn tg-btn-lg" ng-click="sendAcceptance()">Continue</button>
                </form>
              </div>
              <div class="modal-footer">
                <button type="button" ng-click="close('No')" class="btn btn-default" data-dismiss="modal">No</button>                
              </div>
            </div>
          </div>
        </div>
     </script>
