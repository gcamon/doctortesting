                            </div>
                            <div>
                                <label>Social history: </label>
                                <textarea placeholder="write.." ng-model="patient.socialHistory"></textarea>
                            </div>
                            <div>
                                <label>Family history: </label>
                                <textarea placeholder="write.." ng-model="patient.familyHistory"></textarea>
                            </div>
                            <div>
                                <label>Drug history: </label>
                                <textarea placeholder="write.." ng-model="patient.drugHistory"></textarea>
