                            <ul> <!--ng-class="{'drp-list': drop}"-->
                                <li><a href="#/find-specialist">Find a doctor</a></li> <!-- /user/find-specialist -->                         
                                <li><a href="#/drug">Find Drug</a></li><!--/user/find-drug -->
                                <li><a href="#/scan-search">Find Scan</a></li><!--/user/find-scan-test-->
                                <li class="no-bottom"><a href="#/search-test">Find Test</a></li> <!--/user/find-lab-test-->                                              
                            </ul>
                                                
                        </div>
                       
                       
                        <div style="width:65%; text-align:right;display:inline-block" ng-controller = "createRoomController">
                            <div class="msg-box">
                                 <i class="fa fa-bell noti-bell" style="font-size:20px"><span style="display:inline-block;background-color:#996b88;border-radius:14px;padding:7px;left: 14px; top:-14px;position:absolute;color:#fff;font-size:12px;border:none" ng-if="noteLen > 0 " ng-cloak>{{noteLen}}</span></i>

                                 <ul class="all-notify app-drp">
                                        <li> <h6 style="text-align: center">Notification(s)</h6> </li>
                                        {{getPatientId(<%= userInfo.user_id %>, <%= userInfo.firstname %>, <%= userInfo.lastname %>)}}
                                        
                                        <li ng-repeat = "note in allNote" style="text-align: left" class="note-list-li">
                                            <div ng-if="note.type === 'laboratory'" class="note-listings" ng-click="viewNoteLab()">
                                                <div><em style="display:inline-block;padding-left:20px;font-size:12px">{{note.date | date: "mediumDate"}} &nbsp;&nbsp;{{note.date | date : "mediumTime"}}</em></div>
                                                <i class="" style="font-weight:bold"> {{note.message}}</i>
                                            </div>

                                            <div ng-if="note.type === 'radiology'"  class="note-listings" ng-click="viewNoteRadio()">
                                                <div><em style="display:inline-block;padding-left:20px;font-size:12px">{{note.date | date: "mediumDate"}} &nbsp;&nbsp;{{note.date | date : "mediumTime"}}</em></div>
                                                <i class="" style="font-weight:bold"> {{note.message}}</i>
                                            </div>

                                            <div ng-if="note.type === 'pharmacy'"  class="note-listings clearfix" ng-click="viewNotePharmacy(note.note_id)">
                                                <div><em style="display:inline-block;padding-left:20px;font-size:12px">{{note.date | date: "mediumDate"}} &nbsp;&nbsp;{{note.date | date : "mediumTime"}}</em></div>
                                                <i class="icon-pill" style="font-weight:bold"> {{note.message}}</i>
                                                
                                            </div>
                                            
                                        <li>
                                    </ul>
                            </div>
                            <div class="msg-box">
                                <i class="fa fa-envelope" aria-hidden="true" style="font-size:20px;color:#fff;position:relative;top:4px;cursor:pointer" ng-click="showMsg()">
                                <span style="display:inline-block;background-color:#996b88;border-radius:14px;padding:7px;left: 14px; top:-14px;position:absolute;color:#fff;font-size:12px;border:none" ng-if=" msgLen > 0 " ng-cloak> {{msgLen}}</span></i>

                                <ul class="all-notify app-drp">
                                    <li> <h6 style="text-align: center">Message(s)</h6> </li>
                                    <li ng-repeat = "msg in allMsg" class="msg-drp-list" ng-click= "viewMessage(msg.user_id,msg.message_id)" ng-if="msg.category !== 'need_doctor'">                                        
                                        <img ng-src="{{msg.profile_pic_url}}" style="width: 32px; height: 32px">
                                        <div>
                                            <div style="padding:5px 0;font-size:12px;">{{msg.date | date: "mediumDate"}} &nbsp;&nbsp;{{msg.date | date: "mediumTime"}}</div>                   
                                            <strong style="padding-left:0px;font-size:12px;">{{msg.title}} {{msg.firstname}} {{msg.lastname}}</strong>                      
                                            <em style="font-size:12px;">{{msg.message}}</em>
                                        <div>                                        
                                    </li>
                                    <li ng-repeat = "msg in allMsg" class="msg-drp-list" ng-click= "viewResponse(msg.user_id,msg.complaint_id)" ng-if="msg.category === 'need_doctor'">                                        
                                        <!--<img ng-src="{{msg.doctor_profile_pic_url}}" style="width: 32px; height: 32px">-->
                                        <i class="fa fa-user-md" style="font-size: 32px"></i>
                                        <div>
                                            <small style="display:block;text-align: left">Ref {{msg.complaint_id}}&nbsp;&nbsp;&nbsp;&nbsp;{{msg.date | date: "mediumDate"}}</small>                  
                                            <em>{{msg.message}}</em>
                                        <div>                                        
                                    </li>

                                    <% for(var i = userInfo.patient_mail.length -1; i >= 0; i--) { %>                                                          
                                        <div>{{getData("<%= userInfo.patient_mail[i].doctor_firstname%>","<%= userInfo.patient_mail[i].doctor_lastname %>","<%= userInfo.patient_mail[i].date %>","<%= userInfo.patient_mail[i].doctor_profile_pic_url %>","<%= userInfo.patient_mail[i].consultation_fee %>","<%= userInfo.firstname %>","<%= userInfo.ewallet.available_amount %>","<%= userInfo.patient_mail[i].doctor_id %>","<%= userInfo.patient_mail[i].service_access %>","<%= userInfo.patient_mail[i].doctor_specialty %>","<%= userInfo.patient_mail[i].message %>")}}
                                        </div>                                                
                                            
                                    <% } %>
                                </ul>
                            </div>
                            <div class="msg-box">
                                <i class="fa fa-calendar" aria-hidden="true" style="font-size:20px;color:#fff;position:relative;top:4px;cursor:pointer"><span style="display:inline-block;background-color:#996b88;border-radius:14px;padding:7px;left: 14px; top:-14px;position:absolute;color:#fff;font-size:12px;border:none" ng-if="appLen > 0 " ng-cloak>{{appLen}}</span></i>
                                <ul class="all-notify app-drp">
                                    <li> <h6 style="text-align: center">Appointment(s)</h6> </li>
                                    <li ng-repeat = "app in allApp" class="msg-drp-list" ng-click= "viewAppointment(app.session_id)">                                       
                                        <img ng-src = "{{app.profilePic}}" style="width: 32px; height: 32px">
                                        <div>                   
                                            <span>{{app.title}} {{app.firstname}} {{app.lastname}}</span>                      
                                            <em>Hi, you have an appointment with me.</em>
                                        <div>                                        
                                    </li>                                                         
                                </ul>
                            </div>
                            <div class="msg-box">
                                <i class="fa fa-video-camera" aria-hidden="true" style="font-size:20px;color:#fff;position:relative;top:4px;cursor:pointer" ng-click = "showVideo()"><span style="display:inline-block;background-color:#996b88;border-radius:14px;padding:7px;left: 14px; top:-14px;position:absolute;color:#fff;font-size:12px;border:none" ng-if="videoLen > 0 " ng-cloak>{{videoLen}}</span></i>
                                <ul class="all-notify app-drp">
                                    <li> <h6 style="text-align: center">Video Call Notification</h6> </li>
                                    <li ng-repeat = "call in holdVideoCallResponse" class="msg-drp-list" ng-click= "viewVideoAppointment(call.message_id)">
                                        <div> <!-- use call.profile_pic_url if you want to add pic -->                  
                                            <span>From :  {{call.title}} {{call.firstname}} {{call.lastname}}</span>                      
                                            <em>Hi, your {{call.type}} was granted! conversation holds in less than {{call.time}} minutes.Hang on!</em>
                                        <div>                                        
                                    </li>                                                         
                                </ul>
                            </div>
                           <div class="msg-box">
                                <i class="fa fa-phone" aria-hidden="true" style="font-size:20px;color:#fff;position:relative;top:4px;cursor:pointer"><span style="display:inline-block;background-color:#996b88;border-radius:14px;padding:7px;left: 14px; top:-14px;position:absolute;color:#fff;font-size:12px;border:none" ng-if="<%= userInfo.appointment.length %> > 0 " ng-cloak>{{audioLen}}</span></i>
                                <ul class="all-notify app-drp">
                                    <li> <h6 style="text-align: center">Audio Call Notification</h6> </li>
                                    <li ng-repeat = "call in holdAudioCallResponse" class="msg-drp-list">                                      
                                        
                                        <div>
                                        <!-- use call.profile_pic_url if you want to add pic -->                    
                                            <span>From :  {{call.title}} {{call.firstname}} {{call.lastname}}</span>                      
                                            <em>Hi, you have {{call.type}} appointment with me by <strong>{{call.time}} minutes</strong></em>
                                        <div>                                        
                                    </li>                                                         
                                </ul>
                            </div>
                            <div class="profile-drp">                            
                                <img src="<%= userInfo.profile_pic_url %>" style="width: 32px; height: 32px; border-left: 1px solid grey;border-bottom: 1px solid grey;border-top: 1px solid grey;box-shadow:0px 2px 4px grey;cursor:pointer">
                                <strong style="color:#fff;font-weight: 500;padding: 5px 10px 5px 5px;border-right:2px solid rgba(200,220,220,0.3);border-radius: 2px"><%= userInfo.firstname %> !</strong>
                                <strong style="color:#fff;font-weight: 100;padding: 5px">ID <%= userInfo.user_id %></strong>
                                <ul>
                                    <li><a href="#/edit-picture"> Change profile picture </a></li>
                                    <li><a href="#/edit-profile">Edit profile</a></li>
                                    <li ng-click="logout()">Logout</li>
                                </ul>
                            </div>                            
                        </div>
                    </div>
                </div>
                <div>
                     <div class="request-popup" ng-class="{'pop-out':isReceivedRequest === true}">
                    <img ng-src="{{sender.profile_pic_url}}" style="width:32px; height:32px">
                    <div style="display:inline-block;padding:10px">
                        <strong style="width:80%;display:block;">{{sender.firstname}}!</strong>
                        <i class="fa fa-video-camera" style="color:#33430e;font-size: 18px;" ng-if="sender.type === 'Video Call'"> &nbsp;<span style="color#000">{{sender.type}} Request granted! Starts in less than {{sender.time | date: "mediumTime"}}</span> </i>
                        <i class="fa fa-phone" style="color:#33430e;font-size: 18px;" ng-if="sender.type === 'Audio Call'"> &nbsp;<span style="color#000">{{sender.type}} Request granted! Starts in less than {{sender.time | date: "mediumTime"}}</span> </i>
                        <i class="fa fa-calendar" aria-hidden="true" style="color:#33430e;font-size: 18px;" ng-if="sender.type === 'Meet In-Person'"> &nbsp;<span style="color#000">You have new meet-in-person appointment!</span> </i>
                        <i class="fa fa-envelope" aria-hidden="true" style="color:#33430e;font-size: 18px;" ng-if="sender.type === 'message'"> &nbsp;<span style="color#000">You have new unread message!</span> </i>
                        <i class="fa fa-bell" aria-hidden="true" style="color:#33430e;font-size: 18px;" ng-if="sender.type === 'notification'"> &nbsp;<span style="color#000">You have new notification!</span> </i>
                    </div>
                </div>  
                </div>                
               
                      
