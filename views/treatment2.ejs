    <script type="text/ng-template" id="calender-template.html">
        <div class="modal fade">
          <div class="modal-dialog">
            <div class="modal-content">
              <div class="modal-header">
              <div class="row">
                <div class="col-sm-12 col-md-12 col-lg-12">
                    <button type="button" class="close" ng-click="close('Cancel')" data-dismiss="modal" aria-hidden="true" style="color:red;font-size: 32px">&times;</button>
                </div>                                     
              </div>
              <div class="row">
                <div class="col-sm-12 col-md-12 col-lg-12">
                    <div class="modal-body">
                        <h5>Book an appointment with patient</h5>                      
                       
                        <div class="calender-header">
                            <i class="fa fa-angle-left" ng-click="previous()"></i>
                            <span>{{month.format("MMMM, YYYY")}}</span>
                            <i class="fa fa-angle-right" ng-click="next()"></i>
                        </div>
                        <div class="week">
                            <span class="day">Sun</span>
                            <span class="day">Mon</span>
                            <span class="day">Tue</span>
                            <span class="day">Wed</span>
                            <span class="day">Thu</span>
                            <span class="day">Fri</span>
                            <span class="day">Sat</span>
                        </div>
                        
                        <div ng-repeat="week in weeks" class="indays">
                            <span class="dates" ng-class="{ today: day.isToday, 'different-month': !day.isCurrentMonth, selected: day.date.isSame(selected) }" ng-click="select(day)" style="cursor:pointer" ng-repeat="day in week.days">{{day.number}}</span>
                        </div>
                        <p><strong>{{dd.format('dddd, MMMM Do YYYY')}} {{treatment.appointment.time | date: "mediumTime"}}</strong></p><address><strong>{{treatment.appointment.address}}</strong></address>                        
                        <div style="text-align:center">

                        <h6>Time of appointment: <br><input type="time" placeholder="Enter time" ng-model="treatment.appointment.time"/></h6>
                        <h6>Vanue <small>(If none,defaults to the address in your profile)</small>: <br><input class="addr" type="address" placeholder="Enter address" ng-model="treatment.appointment.address"></h6>
                            <button ng-click="book()" class="btn btn-primary" style="background-color:#7dbb00">Book</button>
                        </div>
                    </div> 
                </div>
              </div>          
            </div>
          </div>
        </div>
     </script>
    
