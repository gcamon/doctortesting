    <script type="text/ng-template" id="question.html" ng-controller="connectController">
         <div class="modal fade">
          <div class="modal-dialog">
            <div class="modal-content">
              <div class="modal-header">
              <div class="row">
                <button type="button" class="close" ng-click="close('Cancel')" data-dismiss="modal" aria-hidden="true">&times;</button>
                     <h6 style="color: #000; font-size: 12px;text-align: center">{{message}}</h6> 
                    <div class="col-md-9 col-lg-9">
                    <h6 style="margin-top: 50px;text-align:center">What symptom are you experiencing?</h6>
                    </div>
                    <div class="col-md-3 col-lg-3">
                    
                    <img ng-src="{{docInfo.profile_pic_url}}" style="width:100px; height:100px"/>
                    </div>
                </div>
                </h6>               
              </div>a
              <div class="modal-body">                                                   
                    <form class="tg-form-modal tg-form-signin">                        
                            <fieldset>
                                <caption>Describe in summary your illness</caption>
                                <div>
                                    <textarea style="height: 300px; color: black" placeholder="write.." ng-model="patient.message"></textarea>
                                </div>
                        <button class="tg-btn tg-btn-lg" ng-click="getAnswer('<%= userInfo.firstname %>','<%= userInfo.lastname %>','<%= userInfo.profile_pic_url %>',<%=userInfo.user_id %>)">Get answer</button>
                            </fieldset>
                        </form>
              </div>
              <div class="modal-footer">
                <button type="button" ng-click="close('No')" class="btn btn-default" data-dismiss="modal">No</button>
                <button type="button" ng-click="close('Yes')" class="btn btn-primary" data-dismiss="modal">Yes</button>
              </div>
            </div>
          </div>
        </div>
     </script>
     
     <script type="text/ng-template" id="insuficient-fund.html" ng-controller="insuficientFundController">
         <div class="modal fade">
          <div class="modal-dialog">
            <div class="modal-content">
              <div class="modal-header">
              <div class="row">
                <button type="button" class="close" ng-click="close('Cancel')" data-dismiss="modal" aria-hidden="true">&times;</button>                
              </div>
              <div class="modal-body">
                <h4>Sorry! we could not continue with your booking</h4>
                <h5 style="font-size: 18px; font-weight: 100">Reason:</5>
                <p style="font-size: 12px; color:#000">You do not have enough fund in your wallet to pay for the doctors consultation fee</p>
                <h5 style="font-size: 18px; font-weight: 100">Details:</5>
                <p style="font-size: 12px;color:#000">Your wallet available amount: <em style="font-size:18px">{{walletAmount | currency }}</em></p>
                <p style="font-size: 12px;color:#000">Consultation fee billed to pay: <em style="font-size:18px">{{consultationFee | currency}}</em></p>

                <button type="button" ng-click="close('Yes')" class="btn btn-primary" data-dismiss="modal">Go ahead to fund wallet now</button>
              </div>
              <div class="modal-footer">               
                <button type="button" ng-click="close('Yes')" class="btn btn-primary" data-dismiss="modal">Close</button>
              </div>
            </div>
          </div>
        </div>
     </script>
