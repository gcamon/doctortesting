                        </div>
                        <div class="col-xs-8 col-md-4">                            
                            <ul class="notify">
                                <li>{{getName("<%= userInfo.firstname %>","<%=userInfo.lastname %>","<%= userInfo.user_id %>")}}</li>
                                
                                <li class="noti chat notify-icon">
                                    <!--<i class="fa fa-commenting-o" style="font-size:24px;color:rgba(0,180,0,0.8)"></i>-->
                                    <strong class="signal" ng-if="isLabP" ng-click="viewLabPending()" style="cursor:pointer" title=" {{labLenPending}} Awaiting lab test result">{{labLenPending}}&nbsp;PLab</strong>
                                </li>
                                <li class="noti video notify-icon">
                                    <!--<i class="fa fa-phone" style="font-size:24px;color:rgba(0,180,0,0.8)"></i>-->
                                    <strong class="signal" ng-if="isScanP" ng-click="viewRadioPending()" style="cursor:pointer" title=" {{scanLenPending}} Awaiting scan test result">{{scanLenPending}} &nbsp;PScan</strong>
                                </li>                               
                                <li class="noti note notify-icon">
                                    <i class="fa fa-bell-o" style="font-size:24px;color:rgba(0,180,0,0.8);position:relative"><span style="display:inline-block;width:24px;height:24px;background-color:#fff;left: 15px; top:-23px; border-radius: 15px;
                                        position:absolute;text-align:center;color:#000;" ng-if="<%= userInfo.patient_notification.length %> > 0 "><%= userInfo.patient_notification.length %></span></i>
                                    <ul class="all-notify">
                                        <li> <h6 style="text-align: center">Notification(s)</h6> </li>
                                        {{getPatientId("<%= userInfo.user_id %>", "<%= userInfo.firstname %>", "<%= userInfo.lastname %>")}}
                                        <% for(var i = userInfo.patient_notification.length -1; i >= 0; i--) { %>
                                            <li class="note-field" ng-click="viewNote('<%= userInfo.patient_notification[i].doctor_id %>')">                                                
                                                <img src="<%= userInfo.patient_notification[i].doctor_profile_pic_url %>" style="width: 50px; height: 50px">
                                                <div>{{getData("<%= userInfo.patient_notification[i].doctor_firstname%>","<%= userInfo.patient_notification[i].doctor_lastname %>","<%= userInfo.patient_notification[i].date %>","<%= userInfo.patient_notification[i].doctor_profile_pic_url %>","<%= userInfo.patient_notification[i].consultation_fee %>","<%= userInfo.firstname %>","<%= userInfo.ewallet.available_amount %>","<%= userInfo.patient_notification[i].doctor_id %>","<%= userInfo.patient_notification[i].service_access %>","<%= userInfo.patient_notification[i].doctor_specialty %>","<%= userInfo.patient_notification[i].message %>")}}
                                                </div>

                                                <span>{{convertDate('<%= userInfo.patient_notification[i].date %>')}}</span>    

                                                <span> From Dr. <%= userInfo.patient_notification[i].doctor_firstname %> <%= userInfo.patient_notification[i].doctor_lastname %></span>
                                                <span style="display:block">{{realDate | date: "medium"}}</span>
                                            </li>
                                        <% } %>

                                    </ul>
                                </li>
                                 <li class="noti chat">
                                    <img src="<%= userInfo.profile_pic_url %>" style= "width:35px;height:35px;border-radius: 20px">
                                    <p style="color:rgba(255,255,255,0.8);
                                    padding-left: 20px"><%= userInfo.firstname %> <%=userInfo.lastname %></p>
                                    <ul class="all-notify">
                                        <li style="text-align:center; cursor:pointer" ng-click="logout()">SIGNOUT</a></li>                                       
                                    </ul>
                                </li>
